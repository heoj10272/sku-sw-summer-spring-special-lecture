package day03;

public class Test {
	public static void main(String[] args) {
		
		Parent p = new Parent();
		System.out.println(p.i);
		System.out.println(p.geti());
		
	
		Child c = new Child();
		System.out.println(c.i);
		System.out.println(c.geti());
		
		Parent pc = new Child();
		System.out.println(pc.i);
		System.out.println(pc.geti());
	}
}
