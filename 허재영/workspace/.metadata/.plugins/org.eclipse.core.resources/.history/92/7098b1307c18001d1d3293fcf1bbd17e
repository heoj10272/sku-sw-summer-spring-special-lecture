package day03;

public class LottoTest {
	
	public static void main(String[] args) {
		int Lotto[] = new int[6];
		int Random = 0;
		
		for (int i=0; i<6;i++) {
			
			while(true) {
				Random = (int)(Math.random()*100%45);
				
				if (Random == 0) {
					continue;
				}
				
				for (int j = 0; j<5;j++) {
					if (Random == Lotto[j]) {
						continue;
					}
				}
				
				break;
			}
			
			Lotto[i] = Random;
		}
		
		System.out.println("");
		for (int k = 0; k<6;k++) {
			System.out.printf("%d ", Lotto[k]);
		}
	}
}
